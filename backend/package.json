{
  "name": "fabrikator-backend",
  "version": "1.0.0",
  "description": "Backend server for Fabrikator project",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon --config nodemon.json --exec ts-node src/server.ts",
    "worker": "ts-node src/workers/index.ts",
    "build": "tsc",
    "test": "jest --passWithNoTests --testPathIgnorePatterns=tools/describeimage/test",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "describe:image": "node scripts/describe-image.js",
    "stl:gen": "python3 scripts/generate_stl.py --analysis scripts/analysis_latest.json --parts scripts/parts_latest.json --out scripts/out",
    "create:account": "ts-node scripts/create-account.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.6.7",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.7.11",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "openai": "^5.15.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.2.5",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^9.0.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prisma": "^5.20.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
