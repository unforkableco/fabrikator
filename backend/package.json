{
  "name": "fabrikator-backend",
  "version": "1.0.0",
  "description": "Backend server for Fabrikator project",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon --config nodemon.json --exec ts-node src/server.ts",
    "worker": "ts-node src/workers/index.ts",
    "build": "tsc",
    "test": "jest",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "cad:gen": "ts-node tools/generate_and_run.ts tools/analysis_latest.txt --image backend/tools/image.png",
    "cad:gen-experimental": "ts-node tools/generate_and_run_experimental.ts tools/analysis_latest.txt --image backend/tools/image.png",
    "cad:gen-experimental-resume": "ts-node tools/generate_and_run_experimental.ts tools/analysis_latest.txt --image backend/tools/image.png --resume",
    "cad:spec": "ts-node tools/generate_device_description.ts \"A Quadcopter drone with 4 arms and a central body\"",
    "cad:preview": "ts-node tools/generate_device_image.ts tools/analysis_latest.txt",
    "cad:upload": "ts-node tools/upload_vector_store.ts",
    "scad:demo": "ts-node tools/scad_agent.ts demo",
    "scad:agent": "ts-node tools/scad_agent_loop.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.6.7",
    "bullmq": "^5.7.11",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "ioredis": "^5.4.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "openai": "^6.5.0",
    "pg": "^8.11.3",
    "undici": "^7.15.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^9.0.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prisma": "^5.20.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
